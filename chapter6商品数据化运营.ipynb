   "source": [
    "from sklearn.neighbors import NearestNeighbors\n",
    "X = [[0.0,0.1,0.6],[0.0,1.5,0.3],[1.2,1.6,0.5]]\n",
    "neigh = NearestNeighbors(n_neighbors=1)\n",
    "neigh.fit(X)\n",
    "new_X = [[1.,1.,1.1]]\n",
    "neigh.kneighbors(new_X)"
